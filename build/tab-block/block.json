{
  "apiVersion": 2,
  "name": "everstox/tabs",
  "version": "0.1.0",
  "title": "Tab Layout",
  "category": "design",
  "icon": "index-card",
  "description": "Display content in a tabbed layout",
  "supports": {
    "html": false,
    "align": [
      "full"
    ],
    "anchor": true,
    "color": {
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "keywords": [
    "tab",
    "tabbed content"
  ],
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "tabLabelsArray": {
      "type": "array",
      "default": []
    },
    "tabColor": {
      "type": "string",
      "default": "#00FFAD"
    }
  },
  "textdomain": "everstox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css"
}