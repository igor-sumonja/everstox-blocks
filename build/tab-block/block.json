{
  "apiVersion": 2,
  "name": "everstox/tabs",
  "version": "0.1.0",
  "title": "Tab Layout",
  "category": "design",
  "icon": "index-card",
  "description": "Display content in a tabbed layout",
  "supports": {
    "html": false
  },
  "keywords": [
    "tab",
    "tabbed content"
  ],
  "attributes": {
    "tabLabelsArray": {
      "type": "array",
      "default": []
    },
    "updateChild": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "everstox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css"
}